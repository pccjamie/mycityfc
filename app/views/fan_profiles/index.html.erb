<div id="trigger">Game Day!</div>
<div id="match-day">
	<section>

	<%= @current_time %>
  <p class="alert"><%= alert %></p>


	<%= @schedules_mls %>


	</section>
	<aside class="right">match info
		<details>time, day, etc</details></aside>

</div>


<details open>
<summary><h5>toggle</h5></summary>
<div id="banner">
	<div id="greeting-block-wide">

	<header>
		<h3>Hi <%= current_user.first_name %>!</h3>
		<h1>It's <%= @weather %></h1>
		<h3>at the moment. Looks like a nice day for a game!</h3>
	</header>
	</div>
	
	<section id="my-teams">

	<div class="sleeve">
		
			<div class="team-overview">	
				<header></header>
				<nav></nav>
			</div>

			<div class="team-media">
				<% @myteams.each do |t| %>
				<!-- <div class="team-name"><%#=t.name %></div>-->
				<div class="team-logo"><img src='<%= t.logo %>'/></div/>
				<div class="team-venue"><%= t.image %></div>
				<% end 	%>
			</div>
	
	</div>

	</section>

</div>

</details>
<div class="clearfix"></div>
<section id="my-news">
<div class="sleeve">
<header><h3>NEWS</h3></header>
</div>
</section>
<section id="twitter">
<div class="sleeve">
<header><h3>WHOS ON YOUR TEAM?</h3></header>
<article></article>
</div>
</section>

<div class="clearfix"></div>

<section id="fans">
<div class="sleeve">
<header><h3>FIND A SUPPORTERS CLUB</h3></header>
</div>
</section>